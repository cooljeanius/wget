  echo hello world > conftest.txt
     if test "$cross_compiling" = yes
then :
  case "$host_os" in
                  # Guess no on native Windows.
          mingw*) gl_cv_func_fflush_stdin="guessing no" ;;
          *)      gl_cv_func_fflush_stdin=cross ;;
        esac

