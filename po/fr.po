msgstr "       --certificate-type=TYPE     type du certificat client, PEM ou DER\n"

#: src/main.c:850
msgid "       --private-key=FILE          private key file\n"
msgstr "       --private-key=FICHIER       fichier de clef privée\n"

#: src/main.c:852
