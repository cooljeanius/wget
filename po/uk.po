#: src/main.c:456
msgid "  -o,  --output-file=FILE    log messages to FILE.\n"
msgstr "  -o,  --output-file=ФАЙЛ    записувати повідомлення у ФАЙЛ.\n"

#: src/main.c:458
msgid "  -a,  --append-output=FILE  append messages to FILE.\n"
msgstr "  -a,  --append-output=ФАЙЛ  додавати повідомлення до ФАЙЛу.\n"

#: src/main.c:461
msgid "  -d,  --debug               print lots of debugging information.\n"
msgstr "  -d,  --debug               виводити відлагоджувальні повідомлення.\n"

#: src/main.c:465
msgid "       --wdebug              print Watt-32 debug output.\n"
msgstr "       --wdebug              вивести діагностичні дані у форматі Watt-32.\n"

#: src/main.c:468
msgid "  -q,  --quiet               quiet (no output).\n"
msgstr "  -q,  --quiet               працювати без виведення повідомлень.\n"

#: src/main.c:470
msgid "  -v,  --verbose             be verbose (this is the default).\n"
msgstr "  -v,  --verbose             докладне виведення (типове).\n"

#: src/main.c:472
msgid "  -nv, --no-verbose          turn off verboseness, without being quiet.\n"
msgstr "  -nv, --no-verbose          вимкнути багатослівність.\n"

#: src/main.c:474
msgid "       --report-speed=TYPE   Output bandwidth as TYPE.  TYPE can be bits.\n"
msgstr "       --report-speed=ТИП    вивести ширину каналу у форматі ТИП. ТИПом може бути «bits».\n"

#: src/main.c:476
msgid "  -i,  --input-file=FILE     download URLs found in local or external FILE.\n"
msgstr "  -i,  --input-file=ФАЙЛ     читати URL з локального або зовнішнього файлу.\n"

#: src/main.c:478
msgid "  -F,  --force-html          treat input file as HTML.\n"
msgstr "  -F,  --force-html          трактувати вхідний файл як HTML.\n"

#: src/main.c:480
