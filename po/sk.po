"Parametre povinné pri dlhých voľbách sú povinné aj pre skrátené voľby.\n"
"\n"

#: src/main.c:442
msgid "Startup:\n"
msgstr "Spustenie:\n"

#: src/main.c:444
msgid "  -V,  --version           display the version of Wget and exit.\n"
msgstr "  -V,  --version           zobraziť verziu programu Wget a skončiť.\n"

#: src/main.c:446
msgid "  -h,  --help              print this help.\n"
msgstr "  -h,  --help              vytlačiť túto pomoc.\n"

#: src/main.c:448
msgid "  -b,  --background        go to background after startup.\n"
msgstr "  -b,  --background        prejsť do pozadia po spustení.\n"

#: src/main.c:450
msgid "  -e,  --execute=COMMAND   execute a `.wgetrc'-style command.\n"
msgstr "  -e,  --execute=PRÍKAZ    vykonať príkaz štýlu .wgetrc.\n"

#: src/main.c:454
msgid "Logging and input file:\n"
msgstr "Zaznamenávanie a vstupný súbor:\n"

#: src/main.c:456
msgid "  -o,  --output-file=FILE    log messages to FILE.\n"
msgstr "  -o,  --output-file=SÚBOR   zaznamenať správy do SÚBORu.\n"

#: src/main.c:458
msgid "  -a,  --append-output=FILE  append messages to FILE.\n"
msgstr "  -a,  --append-output=SÚBOR pridať správy do SÚBORu.\n"

#: src/main.c:461
msgid "  -d,  --debug               print lots of debugging information.\n"
msgstr "  -d,  --debug               vytlačiť množstvo ladiacich informácií.\n"

#: src/main.c:465
msgid "       --wdebug              print Watt-32 debug output.\n"
msgstr "       --wdebug              vytlačiť ladiaci výstup Watt-32.\n"

#: src/main.c:468
msgid "  -q,  --quiet               quiet (no output).\n"
msgstr "  -q,  --quiet               potichu (bez výstupu).\n"

#: src/main.c:470
msgid "  -v,  --verbose             be verbose (this is the default).\n"
msgstr "  -v,  --verbose             byť táravý (toto je štandard).\n"

#: src/main.c:472
msgid "  -nv, --no-verbose          turn off verboseness, without being quiet.\n"
msgstr "  -nv, --no-verbose          vypnúť táravosť bez toho, aby bolo ticho.\n"

#: src/main.c:474
msgid "       --report-speed=TYPE   Output bandwidth as TYPE.  TYPE can be bits.\n"
msgstr "       --report-speed=TYP    Vypisovať šírku pásma ako TYP. TYP môže byť `bits'.\n"

#: src/main.c:476
msgid "  -i,  --input-file=FILE     download URLs found in local or external FILE.\n"
msgstr ""
"  -i,  --input-file=SÚBOR    stiahnuť URL, ktoré sa nachádzajú v miestnom\n"
"                             alebo externom SÚBORe.\n"

#: src/main.c:478
msgid "  -F,  --force-html          treat input file as HTML.\n"
msgstr "  -F,  --force-html          spracovať vstupný súbor ako HTML.\n"

#: src/main.c:480
msgid ""
"  -B,  --base=URL            resolves HTML input-file links (-i -F)\n"
"                             relative to URL.\n"
msgstr ""
"  -B,  --base=URL            prevedie HTML odkazy vstupného súboru (-i -F)\n"
"                             relatívne k URL.\n"

#: src/main.c:483
msgid "       --config=FILE         Specify config file to use.\n"
msgstr "       --config=SÚBOR        Určiť, ktorý konfiguračný súbor použiť.\n"

#: src/main.c:486
msgid "       --metalink-file       download URLs found in local or external metalink FILE.\n"
msgstr ""
"       --metalink-file       stiahnuť URL, ktoré sa nachádzajú v miestnom\n"
"                             alebo externom metalink SÚBORe.\n"

#: src/main.c:491
msgid "Download:\n"
msgstr "Sťahovanie:\n"

#: src/main.c:494
msgid ""
"       --retries                 specify the number of retries for a file.\n"
"                                 (needs to be used with --metalink-file)\n"
msgstr ""
"       --retries                 zadať počet opakovaní pre súbor.\n"
"                                 (potrebné použiť s --metalink-file)\n"

#: src/main.c:498
msgid "  -t,  --tries=NUMBER            set number of retries to NUMBER (0 unlimits).\n"
msgstr "  -t,  --tries=ČÍSLO             nastaviť počet opakovaní na ČÍSLO (0 neobmedzene).\n"

#: src/main.c:500
msgid "       --retry-connrefused       retry even if connection is refused.\n"
msgstr "       --retry-connrefused       pokúsiť sa znova, aj keď bolo spojenie odmietnuté.\n"

#: src/main.c:502
msgid "  -O,  --output-document=FILE    write documents to FILE.\n"
msgstr "  -O,  --output-document=SÚBOR   zapísať dokumenty do SÚBORu.\n"

#: src/main.c:504
msgid ""
"  -nc, --no-clobber              skip downloads that would download to\n"
"                                 existing files (overwriting them).\n"
msgstr ""
"  -nc, --no-clobber              preskočiť sťahovania, ktoré by sťahovali\n"
"                                 do existujúcich súborov (prepísali ich).\n"

#: src/main.c:507
msgid "  -c,  --continue                resume getting a partially-downloaded file.\n"
msgstr "  -c,  --continue                obnoviť získavanie čiastočne stiahnutého súboru.\n"

#: src/main.c:509
msgid "       --progress=TYPE           select progress gauge type.\n"
msgstr "       --progress=TYP            zvoliť typ zobrazenia postupu.\n"

#: src/main.c:511
msgid ""
"  -N,  --timestamping            don't re-retrieve files unless newer than\n"
"                                 local.\n"
msgstr ""
"  -N,  --timestamping            nesťahovať opäť súbory, iba ak sú novšie\n"
"                                 ako miestne.\n"

#: src/main.c:514
msgid ""
"  --no-use-server-timestamps     don't set the local file's timestamp by\n"
"                                 the one on the server.\n"
msgstr ""
"  --no-use-server-timestamps     nenastavovať časové značky miestnych súborov\n"
"                                 podľa toho ako sú na serveri.\n"

#: src/main.c:517
msgid "  -S,  --server-response         print server response.\n"
msgstr "  -S,  --server-response         vytlačiť odpoveď servera.\n"

#: src/main.c:519
msgid "       --spider                  don't download anything.\n"
msgstr "       --spider                  nesťahovať nič.\n"

#: src/main.c:521
msgid "  -T,  --timeout=SECONDS         set all timeout values to SECONDS.\n"
msgstr "  -T,  --timeout=SEKUNDY         nastaviť všetky hodnoty časových limitov na SEKUNDY.\n"

#: src/main.c:523
msgid "       --dns-timeout=SECS        set the DNS lookup timeout to SECS.\n"
msgstr "       --dns-timeout=SEKUNDY     nastaviť časový limit DNS vyhľadávania na SEKUNDY.\n"

#: src/main.c:525
msgid "       --connect-timeout=SECS    set the connect timeout to SECS.\n"
msgstr "       --connect-timeout=SEKUNDY nastaviť časový limit spojenia na SEKUNDY.\n"

#: src/main.c:527
msgid "       --read-timeout=SECS       set the read timeout to SECS.\n"
msgstr "       --read-timeout=SEKUNDY    nastaviť časový limit čítania na SEKUNDY.\n"

#: src/main.c:529
msgid "  -w,  --wait=SECONDS            wait SECONDS between retrievals.\n"
msgstr "  -w,  --wait=SEKUNDY            počkať SEKUNDY medzi sťahovaniami.\n"

#: src/main.c:531
msgid "       --waitretry=SECONDS       wait 1..SECONDS between retries of a retrieval.\n"
msgstr "       --waitretry=SEKÚND        počkať 1..SEKÚND medzi pokusmi o sťahovanie.\n"

#: src/main.c:533
msgid "       --random-wait             wait from 0.5*WAIT...1.5*WAIT secs between retrievals.\n"
msgstr ""
"       --random-wait             počkať od 0,5 × POČKAŤ ... 1,5 × POČKAŤ sekúnd medzi\n"
"                                 sťahovaniami.\n"

#: src/main.c:535
msgid "       --no-proxy                explicitly turn off proxy.\n"
msgstr "       --no-proxy                explicitne vypnúť proxy.\n"

#: src/main.c:537
msgid "  -Q,  --quota=NUMBER            set retrieval quota to NUMBER.\n"
msgstr "  -Q,  --quota=ČÍSLO             nastaviť limit sťahovania na ČÍSLO.\n"

#: src/main.c:539
msgid "       --bind-address=ADDRESS    bind to ADDRESS (hostname or IP) on local host.\n"
msgstr "       --bind-address=ADRESA     zviazať s ADRESOU (názov hostiteľa alebo IP) na miestnom hostiteľovi.\n"

#: src/main.c:541
msgid "       --limit-rate=RATE         limit download rate to RATE.\n"
msgstr "       --limit-rate=RÝCHLOSŤ     obmedziť rýchlosť sťahovania na RÝCHLOSŤ.\n"

#: src/main.c:543
msgid "       --no-dns-cache            disable caching DNS lookups.\n"
msgstr "       --no-dns-cache            zakázať dočasné ukladanie DNS vyhľadávania.\n"

#: src/main.c:545
msgid "       --restrict-file-names=OS  restrict chars in file names to ones OS allows.\n"
msgstr "       --restrict-file-names=OS  obmedziť znaky v názvoch súborov na tie, ktoré povoľuje OS.\n"

#: src/main.c:547
msgid "       --ignore-case             ignore case when matching files/directories.\n"
msgstr "       --ignore-case             ignorovať veľkosť písmen pri porovnávaní súborov/adresárov.\n"

#: src/main.c:550
msgid "  -4,  --inet4-only              connect only to IPv4 addresses.\n"
msgstr "  -4,  --inet4-only              pripájať sa len na adresy IPv4.\n"

#: src/main.c:552
msgid "  -6,  --inet6-only              connect only to IPv6 addresses.\n"
msgstr "  -6,  --inet6-only              pripájať sa len na adresy IPv6.\n"

#: src/main.c:554
msgid ""
"       --prefer-family=FAMILY    connect first to addresses of specified family,\n"
"                                 one of IPv6, IPv4, or none.\n"
msgstr ""
"       --prefer-family=RODINA    pripájať sa najskôr k adresám zadanej rodiny,\n"
"                                 jedno z IPv6, IPv4 alebo none.\n"

#: src/main.c:558
msgid "       --user=USER               set both ftp and http user to USER.\n"
msgstr "       --user=POUŽÍVATEĽ         nastaviť ftp a http používateľov na POUŽÍVATEĽ.\n"

#: src/main.c:560
msgid "       --password=PASS           set both ftp and http password to PASS.\n"
msgstr "       --password=HESLO          nastaviť ftp a http heslo na HESLO.\n"

#: src/main.c:562
msgid "       --ask-password            prompt for passwords.\n"
msgstr "       --ask-password            pýtať sa na heslá.\n"

#: src/main.c:564
msgid "       --no-iri                  turn off IRI support.\n"
msgstr "       --no-iri                  vypnúť podporu IRI.\n"

#: src/main.c:566
msgid "       --local-encoding=ENC      use ENC as the local encoding for IRIs.\n"
msgstr "       --local-encoding=KÓD      použiť KÓD ako miestne kódovanie pre IRI.\n"

#: src/main.c:568
msgid "       --remote-encoding=ENC     use ENC as the default remote encoding.\n"
msgstr "       --remote-encoding=KÓD     použiť KÓD ako predvolené vzdialené kódovanie.\n"

#: src/main.c:570
msgid "       --unlink                  remove file before clobber.\n"
msgstr "       --unlink                  odstrániť súbor pred čistením.\n"

#: src/main.c:573
msgid "       --jobs                    specify how many threads use.\n"
msgstr "       --jobs                    zadať, koľko vláken použiť.\n"

#: src/main.c:577
msgid "Directories:\n"
msgstr "Adresáre:\n"

#: src/main.c:579
msgid "  -nd, --no-directories           don't create directories.\n"
msgstr "  -nd, --no-directories           nevytvárať adresáre.\n"

#: src/main.c:581
msgid "  -x,  --force-directories        force creation of directories.\n"
msgstr "  -x,  --force-directories        vynútiť vytváranie adresárov.\n"

#: src/main.c:583
msgid "  -nH, --no-host-directories      don't create host directories.\n"
msgstr "  -nH, --no-host-directories      nevytvárať adresáre hostiteľa.\n"

#: src/main.c:585
msgid "       --protocol-directories     use protocol name in directories.\n"
msgstr "       --protocol-directories     použiť názov protokolu v adresároch.\n"

#: src/main.c:587
msgid "  -P,  --directory-prefix=PREFIX  save files to PREFIX/...\n"
msgstr "  -P,  --directory-prefix=PREDP   uložiť súbory do PREDP/...\n"

#: src/main.c:589
msgid "       --cut-dirs=NUMBER          ignore NUMBER remote directory components.\n"
msgstr "       --cur-dirs=POČET           ignorovať POČET vzdialených častí názvu adresára.\n"

#: src/main.c:593
msgid "HTTP options:\n"
msgstr "HTTP voľby:\n"

#: src/main.c:595
msgid "       --http-user=USER        set http user to USER.\n"
msgstr "       --http-user=POUŽÍVATEĽ  nastaviť http používateľa na POUŽÍVATEĽ.\n"

#: src/main.c:597
msgid "       --http-password=PASS    set http password to PASS.\n"
msgstr "       --http-password=HESLO   nastaviť http heslo na HESLO.\n"

#: src/main.c:599
msgid "       --no-cache              disallow server-cached data.\n"
msgstr "       --no-cache              nepovoliť dočasne uložené dáta na serveri.\n"

#: src/main.c:601
msgid ""
"       --default-page=NAME     Change the default page name (normally\n"
"                               this is `index.html'.).\n"
msgstr ""
"       --default-page=NÁZOV    Zmeniť názov predvolenej stránky (štandardne\n"
"                               je to `index.html'.).\n"

#: src/main.c:604
msgid "  -E,  --adjust-extension      save HTML/CSS documents with proper extensions.\n"
msgstr "  -E,  --html-extension        uložiť HTML/CSS dokumenty so správnou príponou.\n"

#: src/main.c:606
msgid "       --ignore-length         ignore `Content-Length' header field.\n"
msgstr "       --ignore-length         ignorovať pole `Content-Length' v hlavičke.\n"

#: src/main.c:608
msgid "       --header=STRING         insert STRING among the headers.\n"
msgstr "       --header=REŤAZEC        vložiť REŤAZEC do hlavičky.\n"

#: src/main.c:610
msgid "       --max-redirect          maximum redirections allowed per page.\n"
msgstr "       --max-redirect          maximum povolených presmerovaní na stránku.\n"

#: src/main.c:612
msgid "       --proxy-user=USER       set USER as proxy username.\n"
msgstr "       --proxy-user=POUŽÍVATEĽ nastaviť POUŽÍVATEĽa ako používateľa proxy.\n"

#: src/main.c:614
msgid "       --proxy-password=PASS   set PASS as proxy password.\n"
msgstr "       --proxy-password=HESLO  nastaviť HESLO ako heslo proxy.\n"

#: src/main.c:616
msgid "       --referer=URL           include `Referer: URL' header in HTTP request.\n"
msgstr "       --referer=URL           zahrnúť hlavičku `Referer: URL' do HTTP požiadavky.\n"

#: src/main.c:618
msgid "       --save-headers          save the HTTP headers to file.\n"
msgstr "       --save-headers          uložiť HTTP hlavičky do súboru.\n"

#: src/main.c:620
msgid "  -U,  --user-agent=AGENT      identify as AGENT instead of Wget/VERSION.\n"
msgstr "  -U,  --user-agent=AGENT      identifikovať sa ako AGENT namiesto Wget/VERZIA.\n"

#: src/main.c:622
msgid "       --no-http-keep-alive    disable HTTP keep-alive (persistent connections).\n"
msgstr "       --no-http-keep-alive    zakázať HTTP keep-alive (trvalé spojenia).\n"

#: src/main.c:624
msgid "       --no-cookies            don't use cookies.\n"
msgstr "       --no-cookies            nepoužívať koláčiky.\n"

#: src/main.c:626
msgid "       --load-cookies=FILE     load cookies from FILE before session.\n"
msgstr "       --load-cookies=SÚBOR    pre sedením načítať koláčiky zo SÚBORu.\n"

#: src/main.c:628
msgid "       --save-cookies=FILE     save cookies to FILE after session.\n"
msgstr "       --save-cookies=SÚBOR    po sedení uložiť koláčiky do SÚBORu.\n"

#: src/main.c:630
msgid "       --keep-session-cookies  load and save session (non-permanent) cookies.\n"
msgstr "       --keep-session-cookies  načítať a uložiť koláčiky sedenia (nie trvalé).\n"

#: src/main.c:632
msgid "       --post-data=STRING      use the POST method; send STRING as the data.\n"
msgstr "       --post-data=REŤAZEC     použiť POST metódu; poslať REŤAZEC ako dáta.\n"

#: src/main.c:634
msgid "       --post-file=FILE        use the POST method; send contents of FILE.\n"
msgstr "       --post-file=SÚBOR       použiť POST metódu; poslať obsah SÚBORu.\n"

#: src/main.c:636
msgid "       --method=HTTPMethod     use method \"HTTPMethod\" in the header.\n"
msgstr "       --method=HTTPmetóda     použiť metódu \"HTTPmetóda\" v hlavičke.\n"

#: src/main.c:638
msgid "       --body-data=STRING      Send STRING as data. --method MUST be set.\n"
msgstr "       --body-data=REŤAZEC     Poslať REŤAZEC ako dáta. --method MUSÍ byť nastavené.\n"

#: src/main.c:640
msgid "       --body-file=FILE        Send contents of FILE. --method MUST be set.\n"
msgstr "       --body-file=SÚBOR       Poslať obsah SÚBORu.  --method MUSÍ byť nastavené.\n"

#: src/main.c:642
msgid ""
"       --content-disposition   honor the Content-Disposition header when\n"
"                               choosing local file names (EXPERIMENTAL).\n"
msgstr ""
"       --content-disposition   dodržať hlavičku Content-Disposition pri\n"
"                               voľbe miestnych názvov súborov\n"
"                               (EXPERIMENTÁLNE).\n"

#: src/main.c:645
msgid "       --content-on-error      output the received content on server errors.\n"
msgstr "       --content-on-error      vypisovať prijatý obsah pri chybách servera.\n"

#: src/main.c:647
msgid ""
"       --auth-no-challenge     send Basic HTTP authentication information\n"
"                               without first waiting for the server's\n"
"                               challenge.\n"
msgstr ""
"       --auth-no-challenge     poslať informáciu o základnom overení totožnosti\n"
"                               HTTP bez počiatočného čakania na výzvu servera.\n"

#: src/main.c:654
msgid "HTTPS (SSL/TLS) options:\n"
msgstr "Voľby HTTPS (SSL/TLS):\n"

#: src/main.c:656
msgid ""
"       --secure-protocol=PR     choose secure protocol, one of auto, SSLv2,\n"
"                                SSLv3, and TLSv1.\n"
msgstr ""
"       --secure-protocol=PR     vybrať bezpečný protokol, jeden z auto, SSLv2,\n"
"                                SSLv3 alebo TLSv1.\n"

#: src/main.c:659
msgid "       --no-check-certificate   don't validate the server's certificate.\n"
msgstr "       --no-check-certificate   neoverovať certifikát servera.\n"

#: src/main.c:661
msgid "       --certificate=FILE       client certificate file.\n"
msgstr "       --certificate=SÚBOR      súbor certifikátu klienta.\n"

#: src/main.c:663
msgid "       --certificate-type=TYPE  client certificate type, PEM or DER.\n"
msgstr "       --certificate-type=TYP   typ certifikátu klienta, PEM alebo DER.\n"

#: src/main.c:665
msgid "       --private-key=FILE       private key file.\n"
msgstr "       --private-key=SÚBOR      súbor súkromného kľúča.\n"

#: src/main.c:667
msgid "       --private-key-type=TYPE  private key type, PEM or DER.\n"
msgstr "       --private-key-type=TYP   typ súkromného kľúča, PEM alebo DER.\n"

#: src/main.c:669
msgid "       --ca-certificate=FILE    file with the bundle of CA's.\n"
msgstr "       --ca-certificate=SÚBOR   súbor s balíkom CA.\n"

#: src/main.c:671
msgid "       --ca-directory=DIR       directory where hash list of CA's is stored.\n"
msgstr "       --ca-directory=ADR       adresár, kde je uložený hašovaný zoznam CA.\n"

#: src/main.c:673
msgid "       --random-file=FILE       file with random data for seeding the SSL PRNG.\n"
msgstr "       --random-file=SÚBOR      súbor s náhodnými dátami, pre spustenie SSL PRNG.\n"

#: src/main.c:675
msgid "       --egd-file=FILE          file naming the EGD socket with random data.\n"
msgstr "       --egd-file=SÚBOR         súbor s pomenovaním EGD soketu s náhodnými dátami.\n"

#: src/main.c:680
msgid "FTP options:\n"
msgstr "FTP voľby:\n"

#: src/main.c:683
msgid "       --ftp-stmlf             Use Stream_LF format for all binary FTP files.\n"
msgstr "       --ftp-stmlf             Použiť formát Stream_LF pre všetky binárne súbory FTP.\n"

#: src/main.c:686
msgid "       --ftp-user=USER         set ftp user to USER.\n"
msgstr "       --ftp-user=POUŽÍVATEĽ   nastaviť ftp používateľa na POUŽÍVATEĽ.\n"

#: src/main.c:688
msgid "       --ftp-password=PASS     set ftp password to PASS.\n"
msgstr "       --ftp-password=HESLO    nastaviť ftp heslo na HESLO.\n"

#: src/main.c:690
msgid "       --no-remove-listing     don't remove `.listing' files.\n"
msgstr "       --no-remove-listing     neodstraňovať súbory `.listing'.\n"

#: src/main.c:692
msgid "       --no-glob               turn off FTP file name globbing.\n"
msgstr "       --no-glob               pri FTP vypnúť používanie divokých znakov v názvoch súborov.\n"

#: src/main.c:694
msgid "       --no-passive-ftp        disable the \"passive\" transfer mode.\n"
msgstr "       --no-passive-ftp        zakázať \"pasívny\" režim prenosu.\n"

#: src/main.c:696
msgid "       --preserve-permissions  preserve remote file permissions.\n"
msgstr "       --preserve-permissions  zachovať prístupové práva vzdialeného súboru.\n"

#: src/main.c:698
msgid "       --retr-symlinks         when recursing, get linked-to files (not dir).\n"
msgstr "       --retr-symlinks         pri rekurzii získať spojené súbory (nie adresáre).\n"

#: src/main.c:702
msgid "WARC options:\n"
msgstr "WARC voľby:\n"

#: src/main.c:704
msgid "       --warc-file=FILENAME      save request/response data to a .warc.gz file.\n"
msgstr "       --warc-file=NÁZOVSÚBORU   uložiť údaje o požiadavkách/odpovediach do súboru .warc.gz.\n"

#: src/main.c:706
msgid "       --warc-header=STRING      insert STRING into the warcinfo record.\n"
msgstr "       --warc-header=REŤAZEC     vložiť REŤAZEC do záznamu warcinfo.\n"

#: src/main.c:708
msgid "       --warc-max-size=NUMBER    set maximum size of WARC files to NUMBER.\n"
msgstr "       --warc-max-size=ČÍSLO     nastaviť maximálnu veľkosť WARC súborov na ČÍSLO.\n"

#: src/main.c:710
msgid "       --warc-cdx                write CDX index files.\n"
msgstr "       --warc-cdx                zapísať indexové súbory CDX.\n"

#: src/main.c:712
msgid "       --warc-dedup=FILENAME     do not store records listed in this CDX file.\n"
msgstr "       --warc-dedup=NÁZOVSÚBORU  neukladať záznamy uvedené v tomto CDX súbore.\n"

#: src/main.c:715
msgid "       --no-warc-compression     do not compress WARC files with GZIP.\n"
msgstr "       --no-warc-compression     nekomprimovať súbory WARC pomocou GZIP.\n"

#: src/main.c:718
msgid "       --no-warc-digests         do not calculate SHA1 digests.\n"
msgstr "       --no-warc-digests         nepočítať kontrolný súčet SHA-1.\n"

#: src/main.c:720
msgid "       --no-warc-keep-log        do not store the log file in a WARC record.\n"
msgstr "       --no-warc-keep-log        neukladať súbor so záznamom do WARC záznamu.\n"

#: src/main.c:722
msgid ""
"       --warc-tempdir=DIRECTORY  location for temporary files created by the\n"
"                                 WARC writer.\n"
msgstr ""
"       --warc-tempdir=ADRESÁR    umiestnenie dočasných súborov vytvorených\n"
"                                 WARC zapisovačom.\n"

#: src/main.c:727
msgid "Recursive download:\n"
msgstr "Rekurzívne sťahovanie:\n"

#: src/main.c:729
msgid "  -r,  --recursive          specify recursive download.\n"
msgstr "  -r,  --recursive          nastaviť rekurzívne sťahovanie.\n"

#: src/main.c:731
msgid "  -l,  --level=NUMBER       maximum recursion depth (inf or 0 for infinite).\n"
msgstr "  -l,  --level=ČÍSLO        maximálna hĺbka rekurzie (inf alebo 0 pre nekonečno).\n"

#: src/main.c:733
msgid "       --delete-after       delete files locally after downloading them.\n"
msgstr "       --delete-after       odstrániť miestne súbory po ich stiahnutí.\n"

#: src/main.c:735
msgid ""
"  -k,  --convert-links      make links in downloaded HTML or CSS point to\n"
"                            local files.\n"
msgstr ""
"  -k,  --convert-links      zmeniť odkazy v stiahnutých HTML a CSS tak,\n"
"                            aby ukazovaly na miestne súbory.\n"

#: src/main.c:739
msgid "  -K,  --backup-converted   before converting file X, back up as X_orig.\n"
msgstr "  -K,  --backup-converted   pred konverziou súboru X ho zazálohovať ako X_orig.\n"

#: src/main.c:742
msgid "  -K,  --backup-converted   before converting file X, back up as X.orig.\n"
msgstr "  -K,  --backup-converted   pred konverziou súboru X ho zazálohovať ako X.orig.\n"

#: src/main.c:745
msgid "  -m,  --mirror             shortcut for -N -r -l inf --no-remove-listing.\n"
msgstr "  -m,  --mirror             skratka pre -N -r -l inf --no-remove-listing.\n"

#: src/main.c:747
msgid "  -p,  --page-requisites    get all images, etc. needed to display HTML page.\n"
msgstr "  -p,  --page-requisites    získať všetky obrázky, atď. potrebné pre zobrazenie HTML stránky.\n"

#: src/main.c:749
msgid "       --strict-comments    turn on strict (SGML) handling of HTML comments.\n"
msgstr "       --strict-comments    zapnúť striktné (SGML) spracovávanie HTML komentárov.\n"

#: src/main.c:753
msgid "Recursive accept/reject:\n"
msgstr "Rekurzívne akceptovanie/odmietnutie:\n"

#: src/main.c:755
msgid "  -A,  --accept=LIST               comma-separated list of accepted extensions.\n"
msgstr "  -A,  --accept=ZOZNAM             čiarkou oddelený zoznam akceptovaných prípon.\n"

#: src/main.c:757
msgid "  -R,  --reject=LIST               comma-separated list of rejected extensions.\n"
msgstr "  -R,  --reject=ZOZNAM             čiarkou oddelený zoznam odmietnutých prípon.\n"

#: src/main.c:759
msgid "       --accept-regex=REGEX        regex matching accepted URLs.\n"
msgstr "       --accept-regex=REGVÝRAZ     regulárny výraz pre akceptované URL.\n"

#: src/main.c:761
msgid "       --reject-regex=REGEX        regex matching rejected URLs.\n"
msgstr "       --reject-regex=REGVÝRAZ     regulárny výraz je odmietnuté URL.\n"

#: src/main.c:764
msgid "       --regex-type=TYPE           regex type (posix|pcre).\n"
msgstr "       --regex-type=TYP            typ regulárneho výrazu (posix|pcre).\n"

#: src/main.c:767
msgid "       --regex-type=TYPE           regex type (posix).\n"
msgstr "       --regex-type=TYP            typ regulárneho výrazu (posix).\n"

#: src/main.c:770
msgid "  -D,  --domains=LIST              comma-separated list of accepted domains.\n"
msgstr "  -D,  --domains=ZOZNAM            čiarkou oddelený zoznam akceptovaných domén.\n"

#: src/main.c:772
msgid "       --exclude-domains=LIST      comma-separated list of rejected domains.\n"
msgstr "       --exclude-domains=ZOZNAM    čiarkou oddelený zoznam odmietnutých domén.\n"

#: src/main.c:774
msgid "       --follow-ftp                follow FTP links from HTML documents.\n"
msgstr "       --follow-ftp                nasledovať FTP odkazy z HTML dokumentov.\n"

#: src/main.c:776
msgid "       --follow-tags=LIST          comma-separated list of followed HTML tags.\n"
msgstr "       --follow-tags=ZOZNAM        čiarkou oddelený zoznam nasledovaných HTML značiek.\n"

#: src/main.c:778
msgid "       --ignore-tags=LIST          comma-separated list of ignored HTML tags.\n"
msgstr "       --ignore-tags=ZOZNAM        čiarkou oddelený zoznam ignorovaných HTML značiek.\n"

#: src/main.c:780
msgid "  -H,  --span-hosts                go to foreign hosts when recursive.\n"
msgstr "  -H,  --span-hosts                prejsť na cudzích hostiteľov pri rekurzii.\n"

#: src/main.c:782
msgid "  -L,  --relative                  follow relative links only.\n"
msgstr "  -L,  --relative                  nasledovať len relatívne odkazy.\n"

#: src/main.c:784
msgid "  -I,  --include-directories=LIST  list of allowed directories.\n"
msgstr "  -I,  --include-directories=ZOZNAM zoznam povolených adresárov.\n"

#: src/main.c:786
msgid ""
"  --trust-server-names             use the name specified by the redirection\n"
"                                   url last component.\n"
msgstr ""
"  --trust-server-names             použiť názov určený posledným komponentom\n"
"                                   url presmerovania.\n"

#: src/main.c:789
msgid "  -X,  --exclude-directories=LIST  list of excluded directories.\n"
msgstr "  -X,  --exclude-directories=ZOZNAM zoznam vynechaných adresárov.\n"

#: src/main.c:791
msgid "  -np, --no-parent                 don't ascend to the parent directory.\n"
msgstr "  -np, --no-parent                 nevystupovať do rodičovského adresára.\n"

#: src/main.c:794
msgid "Mail bug reports and suggestions to <bug-wget@gnu.org>.\n"
msgstr ""
"Správy o chybách a návrhy na vylepšenie zasielajte na adresu\n"
"<bug-wget@gnu.org> (iba anglicky).\n"
"Komentáre k slovenskému prekladu zasielajte na adresu <sk-i18n@lists.linux.sk>.\n"

