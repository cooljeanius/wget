#, c-format
msgid "%s: %s:%d: unknown token \"%s\"\n"
msgstr "%s: %s:%d: неизвестный маркер «%s»\n"

#: src/netrc.c:486
#, c-format
msgid "Usage: %s NETRC [HOSTNAME]\n"
msgstr "Использование: %s NETRC [ИМЯ_УЗЛА]\n"

