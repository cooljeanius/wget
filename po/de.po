#: src/main.c:612
msgid "       --wdebug                    print Watt-32 debug output\n"
msgstr "       --wdebug                    Watt-32 Debug-Ausgabe anzeigen\n"

#: src/main.c:615
msgid "  -q,  --quiet                     quiet (no output)\n"
msgstr "  -q,  --quiet                     keine Ausgabe von Meldungen\n"

#: src/main.c:617
msgid "  -v,  --verbose                   be verbose (this is the default)\n"
msgstr "  -v,  --verbose                   ausführliche Meldungen (Vorgabe)\n"

#: src/main.c:619
msgid "  -nv, --no-verbose                turn off verboseness, without being quiet\n"
msgstr "  -nv, --no-verbose                weniger Meldungen, aber nicht stumm\n"

#: src/main.c:621
msgid "       --report-speed=TYPE         output bandwidth as TYPE.  TYPE can be bits\n"
msgstr ""
"       --report-speed=TYP          Bandbreite als TYP ausgeben.\n"
