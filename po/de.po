msgstr ""
"  -B,  --base=URL                  Verweise in der HTML Eingabedatei (-i -F)\n"
"                                   relativ zur URL auflösen\n"

#: src/main.c:634
msgid "       --config=FILE               specify config file to use\n"
msgstr "       --config=DATEI              Datei mit der Konfiguration\n"

#: src/main.c:636
msgid "       --no-config                 do not read any config file\n"
msgstr "       --no-config                 keine Konfigrationsdatei verwenden\n"

#: src/main.c:638
msgid "       --rejected-log=FILE         log reasons for URL rejection to FILE\n"
msgstr "       --rejected-log=DATEI        Grund der URL-Ablehnung in DATEI schreiben\n"

#: src/main.c:642
msgid "Download:\n"
msgstr "Download:\n"

#: src/main.c:644
msgid "  -t,  --tries=NUMBER              set number of retries to NUMBER (0 unlimits)\n"
msgstr ""
"  -t,  --tries=ZAHL                Anzahl der Wiederholversuche auf ZAHL setzen\n"
"                                   (0 steht für unbegrenzt)\n"

#: src/main.c:646
msgid "       --retry-connrefused         retry even if connection is refused\n"
msgstr ""
"       --retry-connrefused         Wiederholen, auch wenn der Partner die\n"
"                                     Verbindung abgelehnt hat\n"

