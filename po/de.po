#: src/main.c:623
msgid "  -i,  --input-file=FILE           download URLs found in local or external FILE\n"
msgstr ""
"  -i,  --input-file=DATEI          in lokaler oder externer DATEI\n"
"                                     gelistete URLs holen\n"

#: src/main.c:626
msgid "       --input-metalink=FILE       download files covered in local Metalink FILE\n"
msgstr ""
"       --input-metalink=DATEI      in lokaler Metalink DATEI gelistete\n"
"                                     URLs holen\n"

#: src/main.c:629
msgid "  -F,  --force-html                treat input file as HTML\n"
msgstr "  -F,  --force-html                Eingabe-Datei als HTML behandeln\n"

#: src/main.c:631
msgid ""
"  -B,  --base=URL                  resolves HTML input-file links (-i -F)\n"
